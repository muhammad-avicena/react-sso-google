{
    "rewrites": [
      {"source": "/(.*)", "destination": "/"}
    ],
    "headers": [

        {
        
        "source": "/(.*)",
        
        "headers": [
        
        {
        
        "key": "Referrer-Policy",
        
        "value": "no-referrer-when-downgrade"
        
        },
        {
            "key": "Strict-Transport-Security",
            "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self'; style-src 'self' 'unsafe-inline'; img-src https://*; child-src 'none';"
        }, 
        {
        
        "key": "X-Content-Type-Options",
        
        "value": "nosniff"
        
        },
        
        {
        
        "key": "X-Frame-Options",
        
        "value": "DENY"
        
        },
        
        {
        
        "key": "X-XSS-Protection",
        
        "value": "1; mode=block"
        
        },
        {
        "key": "X-Robots-Tag",
        "value": "noindex"
        }
        ]
        }
        ]
  }
  